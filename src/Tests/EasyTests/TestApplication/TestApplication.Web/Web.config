<configuration>
  <configSections>
    <sectionGroup name="devExpress">
      <section name="compression" requirePermission="false" type="DevExpress.Web.CompressionConfigurationSection, DevExpress.Web.v21.2, Version=21.2.5.0-beta.0-beta.0-beta.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a"/>
